
@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<h2>Emlakçı Kayıt Ekle</h2>

<div class="form">
    <form class="form-validate form-horizontal" id="feedback_form" method="post" action="/RealEstate/Create/" enctype="multipart/form-data">
        <div class="form-group ">
            <label for="CompanyName" class="control-label col-lg-2">Firma Adı <span class="required">*</span></label>
            <div class="col-lg-10">
                <input class="form-control" id="CompanyName" name="CompanyName" type="text" required />
            </div>
            <label for="Username" class="control-label col-lg-2">Kullanıcı Adı<span class="required">*</span></label>
            <div class="col-lg-10">
                <input class="form-control" id="Username" name="Username" type="text" required />
            </div>
          
        </div>

        <div class="form-group">
            <div class="col-lg-offset-2 col-lg-10">
                <button class="btn btn-primary" type="submit">Ekle</button>
                <button class="btn btn-default" type="button"><a href="/Admin/Index">İptal</a></button>
            </div>
        </div>
    </form>
</div>